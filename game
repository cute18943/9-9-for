#include<iostream>
#include<iomanip>
using namespace std;

int main(){

int a=0,b=0,c=0,d=0,e=0,f=0,g=0,h=0,i=0;

for(a=1;a<=9;a++){
for(d=1;d<=9;d++){
cout<<d;
for(e=1;e<=9-8;e++){
cout<<"*";
for(b=1;b<=9-8;b++){
cout<<a;
for(c=1;c<=9-8;c++){
cout<<"=";
for(g=1;g<=9-8;g++){
cout<<setw(2)<<a*d;
for(h=1;h<=9-8;h++){
cout<<" ";
}
}
}
}
}
}
cout<<endl;
}
return 0;
}
